{
  "name": "cordova-windows",
  "version": "8.0.0-dev",
  "description": "cordova-windows release",
  "bin": "bin/create",
  "main": "template/cordova/Api.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/apache/cordova-windows"
  },
  "bugs": {
    "url": "https://github.com/apache/cordova-windows/issues"
  },
  "keywords": [
    "windows",
