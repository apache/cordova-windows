    "node-uuid": "^1.4.8",
    "nopt": "^4.0.1",
    "q": "^1.5.1",
    "semver": "^5.5.0",
    "shelljs": "^0.5.3",
    "winjs": "^4.4.3"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jasmine": "^3.5.0",
    "nyc": "^15.0.1",
    "rewire": "^5.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
